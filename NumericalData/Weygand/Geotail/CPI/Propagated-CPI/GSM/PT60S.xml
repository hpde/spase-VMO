          <ParameterKey>Hour</ParameterKey>
        </Element>
        <Element>
          <Name>Minute</Name>
          <Index>5</Index>
          <ParameterKey>Minute</ParameterKey>
        </Element>
        <Element>
          <Name>Second</Name>
          <Index>6</Index>
          <ParameterKey>Second</ParameterKey>
        </Element>
      </Structure>
