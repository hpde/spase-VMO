        <TemporalDescription>
            <TimeSpan>
                <StartDate>1995-01-01T00:00:00</StartDate>
                <EndDate>2009-10-20T00:00:00</EndDate>
                <Note>Time format in data files is: Day Month Year Hour Minute Second (DD MM YYYY HH MM SS.SSS)</Note>
            </TimeSpan>
            <Cadence>PT60S</Cadence>
        </TemporalDescription>
        <Caveats>Flags=1.000e+34</Caveats>
        <Keyword>1 min</Keyword>
        <Keyword>solar wind</Keyword>
        <Keyword>magnetotail</Keyword>
        <Keyword>bow shock</Keyword>
        <Keyword>magnetopause</Keyword>
        <Keyword>plasma data</Keyword>
        <Keyword>LEP</Keyword>
        <Keyword>propagated solar wind data</Keyword>
        <Keyword>Geotail</Keyword>
        <PhysicalParameter>
            <Name>Universal Time</Name>
            <ParameterKey>Time</ParameterKey>
            <Description>Universal Time AFTER Propagation columns from left to right are: day month year hour minute sec in the format of dd mm yyyy hh mm ss.</Description>
            <Cadence>PT60S</Cadence>
            <Structure>
                <StructureType>Vector</StructureType>
                <Size>6</Size>
                <Description>Universal Time expressed using a 6-column day, month, year, hour, minute, second representation</Description>
                <Element>
                    <Name>Day</Name>
                    <Index>1</Index>
                    <ParameterKey>Day</ParameterKey>
                </Element>
                <Element>
                    <Name>Month</Name>
                    <Index>2</Index>
                    <ParameterKey>Month</ParameterKey>
                </Element>
                <Element>
                    <Name>Year</Name>
                    <Index>3</Index>
                    <ParameterKey>Year</ParameterKey>
                </Element>
                <Element>
                    <Name>Hour</Name>
                    <Index>4</Index>
                    <ParameterKey>Hour</ParameterKey>
                </Element>
                <Element>
                    <Name>Minute</Name>
                    <Index>5</Index>
                    <ParameterKey>Minute</ParameterKey>
                </Element>
                <Element>
                    <Name>Second</Name>
                    <Index>6</Index>
                    <ParameterKey>Second</ParameterKey>
                </Element>
            </Structure>
            <Support>Temporal</Support>
        </PhysicalParameter>
        <PhysicalParameter>
            <Name>Geotail LEP Density interpolated to 1 min resolution</Name>
            <ParameterKey>Density</ParameterKey>
            <Description>Density for Geotail LEP in units of number per cubic centimeter</Description>
            <Cadence>PT60S</Cadence>
            <Units>#/cm^-3</Units>
            <Measured>
                <Particle>
                    <ParticleType>Ion</ParticleType>
                    <ParticleQuantity>NumberDensity</ParticleQuantity>
                </Particle>
            </Measured>
        </PhysicalParameter>
        <PhysicalParameter>
            <Name>Geotail LEP Thermal Speed</Name>
            <ParameterKey>Thermal Speed</ParameterKey>
            <Description>Geotail LEP thermal speed in units of km/s</Description>
            <Caveats>Be sure you understand the instrument</Caveats>
            <Cadence>PT60S</Cadence>
            <Units>KM/S</Units>
            <Measured>
                <Particle>
                    <ParticleType>Ion</ParticleType>
                    <ParticleQuantity>ThermalSpeed</ParticleQuantity>
                </Particle>
            </Measured>
        </PhysicalParameter>
        <PhysicalParameter>
            <Name>Geotail LEP Velocity Vector</Name>
            <ParameterKey>Velocity Vectors</ParameterKey>
            <Description>Flow vector for Geotail LEP in GSM coordinates in units of km/s</Description>
            <Caveats>Be sure you understand the instrument</Caveats>
            <Cadence>PT60S</Cadence>
            <Units>KM/S</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSM</CoordinateSystemName>
            </CoordinateSystem>
            <Structure>
                <StructureType>Vector</StructureType>
                <Size>3</Size>
                <Element>
                    <Name>VX-GSM</Name>
                    <Component>X</Component>
                    <Index>1</Index>
                    <ParameterKey>pvxgsm</ParameterKey>
                </Element>
                <Element>
                    <Name>VY-GSM</Name>
                    <Component>Y</Component>
                    <Index>2</Index>
                    <ParameterKey>pvygsm</ParameterKey>
                </Element>
                <Element>
                    <Name>VZ-GSM</Name>
                    <Component>Z</Component>
                    <Index>3</Index>
                    <ParameterKey>pvzgsm</ParameterKey>
                </Element>
            </Structure>
            <Measured>
                <Particle>
                    <ParticleType>Ion</ParticleType>
                    <ParticleQuantity>Velocity</ParticleQuantity>
                </Particle>
            </Measured>
        </PhysicalParameter>
        <PhysicalParameter>
            <Name>Geotail spacecraft position vector interpolated to 1 min resolution</Name>
            <ParameterKey>Position Vectors</ParameterKey>
            <Description>Location of the spacecraft in GSM coordinates in units of Re</Description>
            <Cadence>PT60S</Cadence>
            <Units>Re</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSM</CoordinateSystemName>
            </CoordinateSystem>
            <Structure>
                <StructureType>Vector</StructureType>
                <Size>3</Size>
                <Element>
                    <Name>X-GSM</Name>
                    <Component>X</Component>
                    <Index>1</Index>
                    <ParameterKey>nxgsm</ParameterKey>
                </Element>
                <Element>
                    <Name>Y-GSM</Name>
                    <Component>Y</Component>
                    <Index>2</Index>
                    <ParameterKey>nygsm</ParameterKey>
                </Element>
                <Element>
                    <Name>Z-GSM</Name>
                    <Component>Z</Component>
                    <Index>3</Index>
                    <ParameterKey>nzgsm</ParameterKey>
                </Element>
            </Structure>
            <Support>Positional</Support>
        </PhysicalParameter>
    </NumericalData>
